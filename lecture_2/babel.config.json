{
  "presets": [
    [
      "@babel/preset-env", 
      {
        "targets": "last 5 Chrome versions, last 5 Firefox versions"
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "overrides": [
    {
      "test": "./node_modules/react-dom/cjs/react-dom.development.js",
      "compact": false
    }
  ]
}
